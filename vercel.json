{
  "version": 2,
  "builds": [
    {
      "src": "api/optimize.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/optimize",
      "dest": "/api/optimize.js"
    },
    {
      "src": "/(.*\\.(css|js|png|jpg|jpeg|webp|svg|ico))",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
